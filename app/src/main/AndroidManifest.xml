<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.zhenquan.mycustomview">

    <application
            android:name=".MyApplication"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".activity.CircleMenuActivity">
        </activity>
        <activity android:name=".activity.ToastActivity">
        </activity>
        <activity android:name=".activity.PathActivity"/>
        <activity android:name=".activity.ShadowHelperAvtivity"/>
        <activity android:name=".activity.CircleDownloadProgressBarActivity"/>
        <activity android:name=".activity.ProgressRingActivity"/>
        <activity android:name=".activity.TextActivity"/>
        <activity android:name=".activity.EditActivity"/>
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.PieViewActivity"/>
    </application>

</manifest>